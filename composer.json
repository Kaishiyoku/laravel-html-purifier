{
  "name": "kaishiyoku/laravel-html-purifier",
  "description": "A HTML filter using htmlpurifier with additional HTML5 rules.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Wiedel",
      "email": "dev@andreas-wiedel.de"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.1.3",
    "illuminate/support": "5.*",
    "ezyang/htmlpurifier": "4.10.0"
  },
  "autoload": {
    "psr-4": {
      "Kaishiyoku\\": "src/Kaishiyoku"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kaishiyoku\\HtmlPurifier\\HtmlPurifierServiceProvider"
      ],
      "aliases": {
        "HtmlPurifier": "Kaishiyoku\\HtmlPurifier\\Facades\\HtmlPurifier"
      }
    }
  }
}
