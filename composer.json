{
  "name": "kaishiyoku/laravel-html-purifier",
  "description": "A HTML filter using htmlpurifier with additional HTML5 rules.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Wiedel",
      "email": "dev@andreas-wiedel.de"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.1.3",
    "illuminate/support": "^5.0|^6.0|^7.0",
    "ezyang/htmlpurifier": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Kaishiyoku\\HtmlPurifier\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Kaishiyoku\\HtmlPurifier\\ServiceProvider"
      ],
      "aliases": {
        "HtmlPurifier": "Kaishiyoku\\HtmlPurifier\\Facade"
      }
    }
  }
}
